{
    "name": "brunfilm",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "site:dev": "vitepress dev site",
        "site:build": "vitepress build site",
        "site:preview": "vitepress preview site",
        "sonar:scanner": "sonar-scanner -D sonar.token='$Env:SONAR_TOKEN' -D project.settings='./sonar-project.properties'",
        "lint": "run-s eslint prettier",
        "lint:fix": "run-s eslint:fix prettier:fix",
        "eslint": "npx eslint ./site ",
        "eslint:fix": "npx eslint ./site --fix",
        "prettier": "prettier ./site ",
        "prettier:fix": "prettier ./site  --write",
        "code:coverage": "npx vitest run --coverage",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:junit": "vitest --reporter=junit"
    },
    "dependencies": {
        "@vue/devtools-api": "^7.4.6",
        "vite": "5.4.9",
        "vue": "^3.5.12"
    },
    "devDependencies": {
        "@codecov/vite-plugin": "^1.2.0",
        "@eslint/js": "^9.12.0",
        "@eslint/markdown": "^6.2.0",
        "@vitest/coverage-v8": "^2.1.2",
        "eslint": "^9.12.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-vue": "^9.29.0",
        "globals": "^15.11.0",
        "jsdoc": "^4.0.3",
        "npm-run-all2": "^6.2.3",
        "prettier": "^3.3.3",
        "sonarqube-scanner": "^4.2.3",
        "vitepress": "1.4.0",
        "vitest": "^2.1.2"
    }
}
